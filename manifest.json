//json
{ "manifest_version": 2,
    "name" : "Youtube Demistifier", //funny pun
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
            "js" : ["./main.js"]
        }
    ], //permissions - does the inability to retrieve our fetches have something to do with this? set this as tutorial says to
   "permissions": [
       "http://www.youtube.com/*",
       "https.www.youtube.com/*"
   ],
   "browser_action": {
       "default_icon": {
        "16": "icon15.png",
        "48": "icon48.png",
        "128": "icon128.png"
        },
        "popup": "popup.html"
    }
}

